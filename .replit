run = "npm run dev"
modules = ["nodejs-20", "web"]

[deployment]
deploymentTarget = "autoscale"
run = ["sh", "-c", "npm run dev"]

[[ports]]
localPort = 5173
externalPort = 80
exposeLocalhost = true

[env]
PORT = "5173"

[nix]
channel = "stable-25_05"

[workflows]
runButton = "Development Server"

[[workflows.workflow]]
name = "Development Server"
author = 25522429
mode = "sequential"

[[workflows.workflow.tasks]]
task = "shell.exec"
args = "npm run dev"
